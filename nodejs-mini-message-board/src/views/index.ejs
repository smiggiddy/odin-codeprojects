
  <%- include('header') %>
	<div class="container msg-feed"> 
	<div class="headline">
	  <h1><a href="new">Join the chat!</a></h1>
	</div>
	  <% msgs.forEach((msg) => { %>
	    <div class="card">
	    <div class="card-metadata">
	     <h3>@<%= msg.username %></h3>
	     <span><%= dateParser(msg.date) %></span>
	     </div>
	     <div class="card-message">
	     	<p><%= msg.message %></p>
	     </div>
	    </div>
	      <% }); %>

</div>

  <%- include('footer') %>

